{
  "name": "@edunext/frontend-app-validation-panel",
  "version": "0.1.0",
  "description": "Frontend application validation panel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-template-application.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edunext/frontend-app-validation-panel#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edunext/frontend-app-validation-panelissues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.2.0",
    "@edx/frontend-component-footer": "npm:@edunext/frontend-component-footer@12.7.0-unesco.1",
    "@edx/frontend-component-header": "^4.7.0",
    "@edx/frontend-platform": "5.3.0",
    "@edx/paragon": "21.1.2",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "core-js": "3.32.1",
    "formik": "^2.4.3",
    "lodash": "^4.17.21",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "8.1.1",
    "react-router": "6.14.2",
    "react-router-dom": "6.14.2",
    "redux": "4.2.1",
    "regenerator-runtime": "0.13.11",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "12.9.10",
    "@edx/reactifex": "^2.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "axios-mock-adapter": "1.20.0",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.6.2"
  }
}
